<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>GpuEx-Runtime.Visual.Native</id>
    <version>1.0.0</version>
    <title>GpuEx Runtime</title>
    <authors>Contoso</authors>
    <owners>Contoso</owners>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This package contains GPU structures, and should be used as a simple runtime libary.</description>
    <releaseNotes></releaseNotes>
    <copyright>Copyright 2010</copyright>
    <tags>native cuda gpu runtime</tags>
    <language>en-US</language>
  </metadata>
  <files>
    <file src="_build\**\Runtime.*.lib" exclude="_build\**\Runtime.TinyTcl.*.lib;_build\**\Runtime.JimTcl.*.lib;" target="build\native\lib" />
    <file src="src\Runtime\*.targets" target="build\native" />
    <file src="src\Runtime\**\*.h" exclude="src\Runtime\_*\**\*;src\Runtime\GL\*" target="build\native\include" />
    <file src="src\Runtime\_content\*" target="content" />
    <file src="src\Runtime\**\*.cpp" exclude="src\Runtime\Debug.*\*;src\Runtime\Release.*\*;src\Runtime\x64\**\*" target="src" />
    <file src="src\Runtime\**\*.cu" exclude="src\Runtime\_*\**\*" target="src" />
    <file src="src\Runtime\**\*.h" exclude="src\Runtime\_*\**\*;src\Runtime\GL\*" target="src" />
  </files>
</package>
